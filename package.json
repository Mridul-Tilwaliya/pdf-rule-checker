{
  "name": "pdf-rule-checker",
  "version": "1.0.0",
  "description": "A web app that checks PDFs against user-defined rules using LLM",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": ["pdf", "llm", "rule-checker"],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mridul-Tilwaliya/pdf-rule-checker.git"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

